{
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2017"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "newLine": "LF",
    "noUnusedLocals": true,
    "removeComments": false,
    "rootDir": ".nowhere",
    "strict": true,
    "target": "es2017"
  }
}
